    <string name="app_descrip">A fast Wiktionary browser and Word-of-day widget</string>
    <string name="app_name">Wiktionary Word of the Day</string>
    <string name="empty_result">No entry found for this word, or problem reading data.</string>
    <string name="lookup_about">About</string>
    <string name="lookup_random">Random</string>
    <string name="lookup_search">Search</string>
    <string name="search_hint">Define word</string>
    <string name="search_label">Wiktionary search</string>
    <string formatted="false" name="template_define_url">"http://en.wiktionary.org/wiki/%s"</string>
    <string formatted="false" name="template_user_agent">"%s/%s (Linux; Android)"</string>
    <string formatted="false" name="template_wotd_archive_title">"Wiktionary:Word_of_the_day/Archive/%s/%s"</string>
    <string formatted="false" name="template_wotd_title">"Wiktionary:Word of the day/%s %s"</string>
    <string name="widget_error">No word of day found</string>
    <string name="widget_loading">"Loading word\nof day\u2026"</string>
